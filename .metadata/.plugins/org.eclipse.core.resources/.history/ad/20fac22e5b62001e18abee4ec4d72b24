package 부품사용하기;

import 부품만들기.TV;

public class 우리집 {

	public static void main(String[] args) {
		// 지금 TV라는 클래스를 import해와서 myTV라는 object(instance)를 가져왔다.
		TV myTv = new TV();
		myTv.ch = 7;
		myTv.vol = 9;
		myTv.onOff = false;
		
		TV yourTv = new TV();
		yourTv.ch = 7;
		yourTv.vol = 12;
		yourTv.onOff = true;
		
		System.out.println(myTv.ch);
	}

}
