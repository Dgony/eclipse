		JFrame f = new JFrame();
		f.setTitle("일기장쓰기 화면");
		f.setSize(650, 10);
		f.setLayout(flow);
		
		JLabel today1 = new JLabel();
		
		JLabel title1 = new JLabel();
		
		JLabel doc1 = new JLabel();
		
		JTextField today2 = new JTextField();
		
		JTextField title2 = new JTextField();
		
		JTextArea doc2 = new JTextArea();
		
		
		

		f.setVisible(true);

	}
	
