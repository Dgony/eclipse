<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>

<script type="text/javascript">

x = 100;
y = 200;
result = x + y; // + 산술연산자 사용, result 선언해서 ram에 데이터 저장
alert(result)

result = result + 100 // ram에 저장된 result를 꺼내서 +100 이후 result에 재할당
alert(result)

alert(result != 300) // ram에 result 변수로 저장된 데이터가 일치하는 비교하는 비교 연산자
alert(result != 400)

</script>

</head>
<body>

</body>
</html>